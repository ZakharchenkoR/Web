@model IList<Data.Domain.Entities.Model>
@{
    ViewData["Title"] = "Index";
}

<div>
    <div>
        <ul>
            @foreach (var item in Model)
            {
                <li>
                    <h2>@item.Name</h2>
                    <h3>@item.Manufacturer.Name</h3>
                    <h4>@item.Price</h4>
                </li>
            }
        </ul>
    </div>
</div>